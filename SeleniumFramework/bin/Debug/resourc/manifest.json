{
  "version": "0.1.0",
  "manifest_version": 2,
  "name": "FakeProxy",
  "permissions": [
    "proxy",
    "tabs",
    "unlimitedStorage",
    "storage",
    "https://stackoverflow.com/",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [ "background.js" ]
  },
  "minimum_chrome_version": "22.0.0"
}